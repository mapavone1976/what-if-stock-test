{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "high",
    "low"
  ],
  "properties": {
    "high": {
      "$id": "#/properties/high",
      "type": "object",
      "title": "The High Schema",
      "required": [
        "date",
        "price",
        "value",
        "shares"
      ],
      "properties": {
        "date": {
          "$id": "#/properties/high/properties/date",
          "type": "string",
          "title": "The Date Schema",
          "default": "",
          "examples": [
            "2018-10-03"
          ],
          "pattern": "^(.*)$"
        },
        "price": {
          "$id": "#/properties/high/properties/price",
          "type": "string",
          "title": "The Price Schema",
          "default": "",
          "examples": [
            "232.07"
          ],
          "pattern": "^(.*)$"
        },
        "value": {
          "$id": "#/properties/high/properties/value",
          "type": "number",
          "title": "The Value Schema",
          "default": 0.0,
          "examples": [
            481.4988359999999
          ]
        },
        "shares": {
          "$id": "#/properties/high/properties/shares",
          "type": "number",
          "title": "The Shares Schema",
          "default": 0.0,
          "examples": [
            2.0747999999999998
          ]
        }
      }
    },
    "low": {
      "$id": "#/properties/low",
      "type": "object",
      "title": "The Low Schema",
      "required": [
        "date",
        "price",
        "value",
        "shares"
      ],
      "properties": {
        "date": {
          "$id": "#/properties/low/properties/date",
          "type": "string",
          "title": "The Date Schema",
          "default": "",
          "examples": [
            "2018-02-08"
          ],
          "pattern": "^(.*)$"
        },
        "price": {
          "$id": "#/properties/low/properties/price",
          "type": "string",
          "title": "The Price Schema",
          "default": "",
          "examples": [
            "155.15"
          ],
          "pattern": "^(.*)$"
        },
        "value": {
          "$id": "#/properties/low/properties/value",
          "type": "number",
          "title": "The Value Schema",
          "default": 0.0,
          "examples": [
            321.90522
          ]
        },
        "shares": {
          "$id": "#/properties/low/properties/shares",
          "type": "number",
          "title": "The Shares Schema",
          "default": 0.0,
          "examples": [
            2.0747999999999998
          ]
        }
      }
    }
  }
}